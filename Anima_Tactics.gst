                <profile id="eea1-4e75-9952-5ab2" name="Al-Djinn (Lesser): Absolute Change" hidden="false" typeId="f2b7-d01c-fb53-acfd" typeName="Abilities">
                  <characteristics>
                    <characteristic name="Category" typeId="ab25-c14e-2680-78b9">Magic</characteristic>
