        <profile id="4b52-6759-e560-aa12" name="Iosara: Specter" hidden="false" typeId="a395-6b61-f251-e9d3" typeName="Innate Abilities">
          <modifiers>
            <modifier type="set" field="hidden" value="true">
              <conditions>
                <condition field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="315f-ecc8-e4d9-6061" type="atLeast"/>
